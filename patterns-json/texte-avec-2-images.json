  "content": "<!-- wp:group {\"tagName\":\"section\",\"className\":\"relative flex\"} -->\n<section class=\"wp-block-group relative flex\" id=\"texte-2-images\">\n    \n    <!-- wp:group {\"className\":\"flex column relative\",\"id\":\"block-txt\"} -->\n    <div class=\"wp-block-group flex column relative\" id=\"block-txt\">\n        \n        <!-- wp:group {\"className\":\"block-title\"} -->\n        <div class=\"wp-block-group block-title\">\n            <!-- wp:heading {\"level\":2,\"className\":\"title-block-wrapper\"} -->\n            <h2 class=\"wp-block-heading title-block-wrapper\">\n                <span>Titre principal</span>\n                <span>de la section</span>\n            </h2>\n            <!-- /wp:heading -->\n        </div>\n        <!-- /wp:group -->\n        \n        <!-- wp:paragraph {\"className\":\"description\"} -->\n        <p class=\"description\">Description de la section. Vous pouvez modifier ce texte selon vos besoins. Cette section présente un contenu avec un titre stylisé, une description et un bouton d'action.</p>\n        <!-- /wp:paragraph -->\n        \n        <!-- wp:buttons {\"layout\":{\"type\":\"flex\",\"justifyContent\":\"flex-start\"}} -->\n        <div class=\"wp-block-buttons\">\n            <!-- wp:button {\"className\":\"btn btn-outline btn-md\",\"style\":{\"border\":{\"radius\":\"5px\"},\"color\":{\"background\":\"transparent\",\"text\":\"#6b764c\"},\"spacing\":{\"padding\":{\"top\":\"10px\",\"right\":\"30px\",\"bottom\":\"10px\",\"left\":\"30px\"}}}} -->\n            <div class=\"wp-block-button btn btn-outline btn-md\">\n                <a class=\"wp-block-button__link has-text-color has-background wp-element-button\" style=\"border-radius:5px;color:#6b764c;background-color:transparent;padding-top:10px;padding-right:30px;padding-bottom:10px;padding-left:30px\">Découvrir</a>\n            </div>\n            <!-- /wp:button -->\n        </div>\n        <!-- /wp:buttons -->\n        \n    </div>\n    <!-- /wp:group -->\n    \n    <!-- wp:group {\"className\":\"tiles-wrapper\"} -->\n    <div class=\"wp-block-group tiles-wrapper\">\n        \n        <!-- wp:group {\"className\":\"img-wrapper\"} -->\n        <div class=\"wp-block-group img-wrapper\">\n            <!-- wp:image {\"id\":1,\"sizeSlug\":\"medium\",\"linkDestination\":\"none\",\"className\":\"\",\"alt\":\"Première image\"} -->\n            <figure class=\"wp-block-image size-medium\">\n                <img src=\"https://www.chateaudetaulignan.com/wp-content/uploads/2024/06/piscine-03.jpg\" alt=\"Première image\"/>\n            </figure>\n            <!-- /wp:button -->\n        </div>\n        <!-- /wp:group -->\n        \n        <!-- wp:group {\"className\":\"img-wrapper\"} -->\n        <div class=\"wp-block-group img-wrapper\">\n            <!-- wp:image {\"id\":2,\"sizeSlug\":\"medium\",\"linkDestination\":\"none\",\"className\":\"\",\"alt\":\"Deuxième image\"} -->\n            <figure class=\"wp-block-image size-medium\">\n                <img src=\"https://www.chateaudetaulignan.com/wp-content/uploads/2024/06/exterieur-04-2.jpg\" alt=\"Deuxième image\"/>\n            </figure>\n            <!-- /wp:button -->\n        </div>\n        <!-- /wp:group -->\n        \n    </div>\n    <!-- /wp:group -->\n    \n</section>\n<!-- /wp:group -->",
  "description": "Section avec texte à gauche (titre, description et bouton) et deux images à droite",
