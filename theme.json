  "styles": {
    "blocks": {
      "core/buttons": {
        "border": {
          "bottom": {
            "style": "none",
            "width": "0px"
          },
          "color": null,
          "left": {
            "style": "none",
            "width": "0px"
          },
          "radius": "5px",
          "right": {
            "style": "none",
            "width": "0px"
          },
          "style": null,
          "top": {
            "style": "none",
            "width": "0px"
          },
          "width": null
        },
        "spacing": {
          "blockGap": {
            "left": "var(--wp--preset--spacing--small)",
            "top": "var(--wp--preset--spacing--medium)"
          },
          "margin": {
            "bottom": "var(--wp--preset--spacing--large)",
            "top": "var(--wp--preset--spacing--large)"
          },
          "padding": {
            "bottom": "0",
            "left": "0",
            "right": "0",
            "top": "0"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "lineHeight": "1"
        }
      },
      "core/gallery": {
        "spacing": {
          "margin": {
            "top": "4em"
          }
        }
      },
      "core/heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--maghfirea)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontStyle": "normal",
          "fontWeight": "400",
          "lineHeight": "1.5"
        }
      },
      "core/paragraph": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "lineHeight": "1.6"
        }
      },
      "core/post-content": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)"
        }
      },
      "core/post-title": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "core/separator": {
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--60)",
            "top": "var(--wp--preset--spacing--60)"
          }
        },
        "variations": {
          "wide": {
            "color": {
              "background": "var(--wp--preset--color--noir)"
            }
          }
        }
      },
      "core/site-title": {
        "border": {
          "bottom": {
            "style": "none",
            "width": "0px"
          },
          "left": {
            "style": "none",
            "width": "0px"
          },
          "right": {
            "style": "none",
            "width": "0px"
          },
          "top": {
            "style": "none",
            "width": "0px"
          }
        },
        "color": {
          "text": "var(--wp--preset--color--vert-olive)"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--vert-olive)"
            }
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--maghfirea)",
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "woocommerce/product-price": {
        "color": {
          "text": "var(--wp--preset--color--custom-bleu-canard)"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--small)",
            "top": "0"
          },
          "padding": {
            "top": "var(--wp--preset--spacing--small)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontStyle": "normal",
          "fontWeight": "500",
          "lineHeight": "1.2"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--blanc)",
      "text": "var(--wp--preset--color--noir)"
    },
    "elements": {
      "button": {
        "border": {
          "color": "currentColor",
          "style": "solid",
          "width": "1px"
        },
        "color": {
          "background": "var(--wp--preset--color--vert-olive)",
          "text": "#ffffff"
        },
        "elements": {
          "link": {
            ":hover": {
              "border": {
                "color": "currentColor",
                "style": "solid",
                "width": "1px"
              },
              "color": {
                "background": "transparent",
                "text": "var(--wp--preset--color--vert-olive)"
              }
            }
          }
        }
      },
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--maghfirea)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontStyle": "normal",
          "fontWeight": "400",
          "lineHeight": "1.2"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--maghfirea)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontStyle": "normal",
          "fontWeight": "400",
          "lineHeight": "1"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--maghfirea)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontStyle": "normal",
          "fontWeight": "400",
          "lineHeight": "1"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--maghfirea)",
          "fontStyle": "normal",
          "fontWeight": "400"
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--cabin)",
          "fontStyle": "normal",
          "fontWeight": "400",
          "lineHeight": "1.2"
        }
      },
      "link": {
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--olive-fonce)"
          }
        },
        "color": {
          "text": "var(--wp--preset--color--olive)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--bellota-text)",
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontStyle": "normal",
          "fontWeight": "400"
        }
      }
    },
    "spacing": {
      "blockGap": "0"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--bellota-text)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "fontStyle": "normal",
      "fontWeight": "400",
      "lineHeight": "1"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ],
  "version": 3
}
